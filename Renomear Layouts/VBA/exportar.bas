Attribute VB_Name = "exportar"
' ==== Módulo de macros ====
Option Explicit

' Botão: Exportar nomes do Excel para o CAD
Public Sub ExportarParaCAD(control As IRibbonControl)
    MsgBox "Exportando nomes para o CAD..." & vbCrLf & _
           "Aqui entraria sua lógica de integração com o LISP ou via arquivo."
End Sub

' Botão: Importar nomes do CAD para o Excel
Public Sub ImportarDoCAD(control As IRibbonControl)
    MsgBox "Importando nomes do CAD..." & vbCrLf & _
           "Aqui você poderia ler do CAD e preencher planilha."
End Sub


